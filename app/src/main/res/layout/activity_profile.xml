<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile"
    android:background="#253334">

    <FrameLayout
        android:id="@+id/flayout_top"
        android:layout_width="350dp"
        android:layout_height="49dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="42dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/btn_sidemenu"
            android:layout_width="22dp"
            android:layout_height="18dp"
            android:layout_marginTop="15dp"
            app:srcCompat="@drawable/side_menu" />

        <TextView
            android:id="@+id/btn_exit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="325dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/alegreya_regular"
            android:text="exit"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/imageview_logo"
            android:layout_width="50dp"
            android:layout_height="49dp"
            android:layout_marginStart="150dp"
            app:srcCompat="@drawable/home_1" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/flayout_bottom"
        android:layout_width="260dp"
        android:layout_height="25dp"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/btn_home"
            android:layout_width="25dp"
            android:layout_height="25dp"
            app:srcCompat="@drawable/home_0" />

        <ImageView
            android:id="@+id/btn_profile"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="235dp"
            app:srcCompat="@drawable/profile_1" />

        <ImageView
            android:id="@+id/btn_sounds"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="117.5dp"
            app:srcCompat="@drawable/sounds_0" />
    </FrameLayout>

    <ImageView
        android:id="@+id/imageview_profile"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="116dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/coala" />

    <TextView
        android:id="@+id/textview_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="163dp"
        android:layout_marginTop="239dp"
        android:fontFamily="@font/alegreya_regular"
        android:text="Эмиль"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageview_dawn"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="46dp"
        android:layout_marginBottom="330dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dawn" />

    <TextView
        android:id="@+id/textview_time1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:fontFamily="@font/alegreyasans_medium"
        android:text="11:00"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageview_dawn"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/imageview_dawn"
        app:layout_constraintTop_toTopOf="@+id/imageview_dawn"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageview_moon"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="328dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/moon" />

    <TextView
        android:id="@+id/textview_time2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alegreyasans_medium"
        android:text="15:55"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageview_moon"
        app:layout_constraintEnd_toEndOf="@+id/imageview_moon"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="@+id/imageview_moon"
        app:layout_constraintTop_toTopOf="@+id/imageview_moon"
        app:layout_constraintVertical_bias="0.945"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageview_sea"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="208dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/sea"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textview_time3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alegreyasans_medium"
        android:text="19:11"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageview_sea"
        app:layout_constraintEnd_toEndOf="@+id/imageview_sea"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="@+id/imageview_sea"
        app:layout_constraintTop_toTopOf="@+id/imageview_sea"
        app:layout_constraintVertical_bias="0.972"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageview_forest"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="210dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/forest" />

    <TextView
        android:id="@+id/textview_time4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/alegreyasans_medium"
        android:text="11:11"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageview_forest"
        app:layout_constraintEnd_toEndOf="@+id/imageview_forest"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="@+id/imageview_forest"
        app:layout_constraintTop_toTopOf="@+id/imageview_forest"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <FrameLayout
        android:id="@+id/flayout_addimage"
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_marginStart="46dp"
        android:layout_marginBottom="90dp"
        android:background="@drawable/corners"
        android:backgroundTint="#6AAE72"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textview_addimage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="64dp"
            android:layout_marginTop="28dp"
            android:fontFamily="@font/alegreya_extrabold"
            android:text="+"
            android:textColor="#FFFFFF"
            android:textSize="30sp" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>